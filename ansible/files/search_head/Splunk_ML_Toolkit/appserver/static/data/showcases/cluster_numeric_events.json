{
    "Disk": {
        "title": "Cluster Hard Drives by SMART Metrics",
        "type": "cluster_numeric_events",
        "searchStages": [
            {
                "searchString": "| inputlookup disk_failures.csv | search SMART_1_Raw=* | sample 1000",
                "type": "spl",
                "earliestTime": "",
                "latestTime": "",
                "role": "datasource"
            },
            {
                "type": "fit",
                "role": "preprocessing",
                "algorithm": "StandardScaler",
                "featureVariables": [
                    "SMART_1_Raw",
                    "SMART_2_Raw",
                    "SMART_3_Raw",
                    "SMART_4_Raw",
                    "SMART_5_Raw"
                ],
                "algorithmParams": {
                    "with_mean": true,
                    "with_std": true
                }
            },
            {
                "type": "fit",
                "role": "preprocessing",
                "algorithm": "PCA",
                "featureVariables": [
                    "SS_SMART_1_Raw",
                    "SS_SMART_2_Raw",
                    "SS_SMART_3_Raw",
                    "SS_SMART_4_Raw",
                    "SS_SMART_5_Raw"
                ],
                "algorithmParams": {
                    "k": 4
                }
            },
            {
                "type": "fit",
                "algorithm": "KMeans",
                "featureVariables": [
                    "PC_1",
                    "PC_2",
                    "PC_3",
                    "PC_4"
                ],
                "algorithmParams": {
                    "k": 3
                },
                "modelName": "example_hard_drives"
            }
        ]
    },
