            },
            {
                "type": "fit",
                "role": "preprocessing",
                "algorithm": "PCA",
                "featureVariables": [
                    "Adjustable_rate_loans(%)",
                    "Contract_interest_rate(%)",
                    "Effective_interest_rate(%)",
                    "Initial_fees_and_charges(%)",
                    "Loan_to_price_ratio(%)",
                    "Purchase_price",
                    "Term_to_maturity",
                    "Year"
                ],
                "algorithmParams": {
                    "k": 3
                }
            },
            {
                "algorithm": "KMeans",
                "algorithmParams": {
                    "k": 3
                },
                "featureVariables": [],
                "role": "main",
                "targetVariables": [
                    "PC_1",
