    "Server Power": {
        "title": "Predict Server Power Consumption",
        "type": "predict_numeric_fields",
        "searchStages": [
            {
                "searchString": "| inputlookup server_power.csv",
                "type": "spl",
                "earliestTime": "",
                "latestTime": "",
                "role": "datasource"
            },
            {
                "type": "fit",
                "algorithm": "LinearRegression",
                "targetVariable": "ac_power",
                "featureVariables": [
                    "total-unhalted_core_cycles",
